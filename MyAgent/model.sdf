<?xml version="1.0" ?>
<sdf version="1.5">
	<model name="My_Agent">
	<pose> 0 0 0.08 0 0 0</pose>
	<link name="chassis">
        
        <inertial>
            <mass> 20 </mass>
            <inertia>
                <!-- 1/12 * mass * (d^2 + h^2) -->
                <!-- 1/12 * 20 * (0.25^2 + 0.04^2) -->
                <ixx> 0.1068 </ixx>
                <iyy> 0.1068 </iyy>
                <izz> 0.1068 </izz>
            </inertia>
        </inertial>
        
        <collision name="chassis_collision">
		<geometry>
			<box>
				<size> 0.25 0.20 0.04 </size>
			</box>
		</geometry>
		</collision>
		
		<visual name="chassis_visual">
		<geometry>
			<box>
				<size> 0.25 0.20 0.04 </size>
			</box>
		</geometry>
		</visual>	
	</link>

	<static> false </static>
	
	<link name="front_left_wheel">
        <pose> 0.120 0.11 0 0 1.5707 1.5707 </pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.06</radius>
              <length>.02</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.06</radius>
              <length>.02</length>
            </cylinder>
          </geometry>
        </visual>
    </link>
	
	<link name="rear_left_wheel">
        <pose> -0.120 0.11 0 0 1.5707 1.5707 </pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.06</radius>
              <length>.02</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.06</radius>
              <length>.02</length>
            </cylinder>
          </geometry>
        </visual>
    </link>
  
	<link name="front_right_wheel">
        <pose> 0.120 -0.11 0 0 1.5707 1.5707 </pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.06</radius>
              <length>.02</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.06</radius>
              <length>.02</length>
            </cylinder>
          </geometry>
        </visual>
	</link>
	
	<link name="rear_right_wheel">
        <pose> -0.120 -0.11 0 0 1.5707 1.5707 </pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.06</radius>
              <length>.02</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.06</radius>
              <length>.02</length>
            </cylinder>
          </geometry>
        </visual>
    </link>
	
	<!-- Motion Joints -->
	<joint type="revolute" name="front_left_wheel_hinge">
        <pose>0 0 -0.0175 0 0 0</pose>
        <child>front_left_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
	</joint>
	
	<joint type="revolute" name="rear_left_wheel_hinge">
        <pose>0 0 -0.0175 0 0 0</pose>
        <child>rear_left_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
	</joint>
	
	<joint type="revolute" name="front_right_wheel_hinge">
        <pose>0 0 0.0175 0 0 0</pose>
        <child>front_right_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
	</joint>

	<joint type="revolute" name="rear_right_wheel_hinge">
        <pose>0 0 0.0175 0 0 0</pose>
        <child>rear_right_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
	</joint>
	
	<!-- Make base for Lidar sensor -->
	<link name="Lidar_Base">
		<pose> 0 0 0.04 0 0 0 </pose>
		<collision name="Lidar Base collision">
			<geometry>
				<box>
					<size> 0.08 0.08 0.04 </size>
				</box>
			</geometry>
		</collision>
		<visual name="Lidar_base_visual">
			<geometry>
				<box>
					<size> 0.08 0.08 0.04 </size>
				</box>
			</geometry>
		</visual>
	</link>
	
	<joint name="Lidar_Base_Joint" type="fixed">
		<pose> 0 0 -0.02 0 0 0 </pose>
		<child> Lidar_Base </child>
		<parent> chassis </parent>
	</joint>

	<!-- Add Lidar sensor -->
	<link name="lidar_link">
		<pose> 0 0 0.075 0 0 0</pose>
		<collision name="lidar_collision">
		<geometry>
			<cylinder>
				<radius>.02</radius>
          		<length>.03</length>
			</cylinder>
		</geometry>
		</collision>

		<visual name="lidar_visual">
		<geometry>
			<cylinder>
				<radius>.02</radius>
          		<length>.03</length>
			</cylinder>
		</geometry>
		</visual>
	
    <!-- This is the ray sensor -->
	<sensor name="Lidar" type="ray">
		<pose>0 0 0 0 0 0</pose>
        <visualize> false </visualize>
		<update_rate> 20 </update_rate>
		<ray>
		  <scan>
            <horizontal>
			  <samples> 360 </samples>
			  <resolution> 1 </resolution>
			  <min_angle> 0 </min_angle>
			  <max_angle> 6.28 </max_angle>
            </horizontal>
            
            <vertical>
			  <samples> 16 </samples>
			  <resolution> 1 </resolution>
			  <min_angle> -0.26 </min_angle>
			  <max_angle> 0.26 </max_angle>
            </vertical>
            
		  </scan>
		  <range>
			  <min> 0.25 </min>
			  <max> 5 </max>
			  <resolution> 20 </resolution>
	  	  </range>
        </ray>
        <!-- Plugin controlling laser data stream
		<plugin name="Imported_plugin" filename="libgazebo_ros_laser.so">
            <topicName> My_Agent/ScanResults </topicName>
            <frameName> lidar </frameName>
        </plugin>
        -->
        <plugin name="Lidar_plugin" filename="libgazebo_ros_block_laser.so">
            <topicName> My_Agent/ScanResults </topicName>
            <frameName> lidar_link  </frameName>
            <gaussianNoise> 0.01 </gaussianNoise>
        </plugin>
        </sensor>
    </link>

	<joint name="lidar_joint" type="fixed">
		<pose> 0 0 0.03 0 0 0 </pose>
		<child> lidar_link </child>
		<parent> Lidar_Base </parent>
	</joint>

    <!-- Velocity Control Plugin -->
	<plugin name="Control_Plugin" filename="libgazebo_ros_skid_steer_drive.so">
		<torque>5.0</torque>
		<leftFrontJoint> front_left_wheel_hinge </leftFrontJoint>
		<rightFrontJoint> front_right_wheel_hinge </rightFrontJoint>
		<leftRearJoint> rear_left_wheel_hinge </leftRearJoint>
		<rightRearJoint> rear_right_wheel_hinge </rightRearJoint>
        <commandTopic> /My_Agent/vel </commandTopic>
        <wheelDiameter> 0.12 </wheelDiameter>
        <wheelSeparation> 0.24 </wheelSeparation>
        <robotBaseFrame> chassis </robotBaseFrame>
            <!-- <odometryFrame> world </odometryFrame> -->
        <broadcastTF> true </broadcastTF>
        <odometryTopic> /My_Agent/odometry </odometryTopic>
        </plugin>
        

    <!--
	<plugin name="Control_Plugin" filename="libgazebo_ros_diff_drive.so">
		<wheelTorque>2.0</wheelTorque>
		<leftJoint> rear_left_wheel_hinge </leftJoint>
		<rightJoint> rear_right_wheel_hinge </rightJoint>
		<commandTopic> /My_Agent/vel </commandTopic>
        <wheelDiameter> 0.12 </wheelDiameter>
        <wheelAcceleration> 3 </wheelAcceleration>
        <wheelSeparation> 0.24 </wheelSeparation>
        <robotBaseFrame> chassis </robotBaseFrame>
        <odometryTopic> /My_Agent/odometry </odometryTopic>
	</plugin>
    --> 

	</model>
</sdf>
